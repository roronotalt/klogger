{
    "name": "@kasssandra/klogger",
    "description": "A simple beautiful logger",
    "author": "Roro",
    "license": "MIT",
    "keywords": [
        "logger",
        "logging",
        "log",
        "console",
        "browser"
    ],
    "main": "dist/index.js",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "type": "module",
